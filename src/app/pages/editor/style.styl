#editor
    overflow hidden

    // .content--wrap
    //     position relative

    #canvas-3d
        width 100%
        height 100%

    .ui-dock-right.animated
        transition-delay 0.1s

    .ui-header
        padding 0 1rem
        line-height 2.5em
        background #293538


// PC icons
@font-face {
    font-family: 'pc-icon';
    src: url("https://s3-eu-west-1.amazonaws.com/static.playcanvas.com/fonts/PlayIcons-Regular.eot");
    src: url("https://s3-eu-west-1.amazonaws.com/static.playcanvas.com/fonts/PlayIcons-Regular.eot?#iefix") format("embedded-opentype"),url("https://s3-eu-west-1.amazonaws.com/static.playcanvas.com/fonts/PlayIcons-Regular.woff2") format("woff2"),url("https://s3-eu-west-1.amazonaws.com/static.playcanvas.com/fonts/PlayIcons-Regular.woff") format("woff"),url("https://s3-eu-west-1.amazonaws.com/static.playcanvas.com/fonts/PlayIcons-Regular.ttf") format("truetype"),url("https://s3-eu-west-1.amazonaws.com/static.playcanvas.com/fonts/PlayIcons-Regular.svg") format("svg");
    font-weight: normal;
    font-style: normal;
}

i.pc.icon, i.pc-icon
    font-family 'pc-icon'
    font-style normal
    font-size 1em
    width 1.715em
    line-height 1.715em

    &.pc-add:before
        content '\E120'

    &.pc-remove:before
        content '\E121'

    &.pc-disable:before
        content '\E132'

    &.pc-enable:before
        content '\E133'

    &.pc-box:before
        content '\E187'

    &.pc-box-filled:before
        content '\E188'

    &.pc-lamp-up:before, &.pc-lamp:before
        content '\E191'

    &.pc-sun:before
        content '\E192'

    &.pc-lamp-down:before
        content '\E193'

    &.pc-lamp-off:before
        content '\E194'

    &.pc-microphone:before
        content '\E196'

    &.pc-speaker:before
        content '\E197'

    &.pc-particles:before
        content '\E199'
